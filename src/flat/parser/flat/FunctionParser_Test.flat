  test `can access parameters from node token matches`() {
    let matcher = FunctionParser().matcher
    let match = matcher.matchAll([
      Token(value: "test", type: Token.Type.IDENTIFIER),
      Token(value: "(", type: Token.Type.SYMBOL),
      Token(value: "Test", type: Token.Type.IDENTIFIER),
      Token(value: "value", type: Token.Type.IDENTIFIER),
      Token(value: ")", type: Token.Type.SYMBOL)
    ], FlatParseContext()).last

    expect(match).toNotBe(null, "Match is null")

    let parameters = FunctionParser().getChildNodeTokenMatches(FlatParseContext(), match)

    expect(parameters).toNotBe(null, "Parameter matches are null")

    expect(parameters).toBe([
      NodeTokenMatch(
        tokens: [
          Token(value: "Test", type: Token.Type.IDENTIFIER),
          Token(value: "value", type: Token.Type.IDENTIFIER)
        ],
        label: "parameter",
        childMatches: [
          NodeTokenMatchChild(
            tokens: [
              Token(value: "Test", type: Token.Type.IDENTIFIER),
              Token(value: "value", type: Token.Type.IDENTIFIER)
            ],
            parser: ParameterParser()
          )
        ]
      )
    ])
  }

