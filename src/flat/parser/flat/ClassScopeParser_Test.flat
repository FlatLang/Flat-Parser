package flat/parser/flat

import flat/parser
import flat/parser/matchers
import flat/ast/FileNode
import flat/ast/AnnotationNode
import flat/annotations/PublicModifier
import flat/annotations/LetModifier
import flat/compiler/models/Token
import flat/io/File

testable class {
  test `does not parse modifier after scope start`() {
    let matcher = ClassScopeParser().matcher
    let result = matcher.matchAll([
      Token(value: "{", type: Token.Type.SYMBOL),
      Token(value: "let", type: Token.Type.IDENTIFIER)
    ], FlatParseContext()).last

    expect(result).toBe(null)
  }
}
