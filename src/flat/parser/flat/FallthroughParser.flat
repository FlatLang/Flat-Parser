package flat/parser/flat

import flat/parser
import flat/parser/matchers
import flat/ast/FallthroughNode
import flat/compiler/models/Token
import flat/log/Logger

data class extends ParserBase {
  static Logger log = Logger(FallthroughParser.class)

  override lazy TokenPattern pattern => SingleTokenPattern(type: Token.Type.IDENTIFIER, value: "fallthrough", exact: true)

  override public generateNode() -> FallthroughNode {
    return FallthroughNode()
  }
}